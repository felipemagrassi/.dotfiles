[help]
    autocorrect=1
[color]
    ui=auto
[user]
    name=Felipe Magrassi
[init]
    defaultBranch=main
[core]
    autocrlf = input
    editor=vim
[pull]
    rebase=false
[push]
    default=current
[status]
    branch=1
    status=1
[commit]
    verbose=true
[format]
    pretty = %C(auto)%h%Creset -%C(auto)%d%Creset %s %C(blue)<%an> %Cgreen(%cr)%Creset
[safe]
	directory = /opt/code-server/data
